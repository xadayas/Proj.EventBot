@model IEnumerable<EventBot.Entities.Service.Models.EventModel>

<h2>Kommande aktiviteter du ska delta i</h2>

<ul class="timeline">
    @foreach(var item in Model)
    {
        <li class="clickable" onclick="document.location = '/event/details/@item.Id';">
            <p class="timeline-date">@Html.DisplayFor(modelItem => item.StartDate)</p>
            <div class="timeline-content">
                <h3 id="eventTitle">@Html.DisplayFor(modelItem => item.Title)</h3>
                <p>@Html.DisplayFor(modelItem => item.MeetingPlace)</p>
            </div>
        </li>
    }
</ul>
