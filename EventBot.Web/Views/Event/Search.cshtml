@model IEnumerable<EventBot.Entities.Service.Models.EventModel>


<table class="table">
  

@foreach (var item in Model) {
    <tr>
        <td>
            <img src="/Images/View/@Html.Raw(item.ImageId)" width="100"/>
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>

        <td>
            @Html.DisplayFor(modelItem => item.MeetingPlace)
        </td>

        <td>
            @Html.ActionLink("Details", "Details", new {id = item.Id})
        </td>
    </tr>
}

</table>
