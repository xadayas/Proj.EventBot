@model IEnumerable<EventBot.Entities.Service.Models.EventModel>


<table class="table">
  

@foreach (var item in Model) {
    <tr class="col-md-12 clickable" onclick="document.location = '/event/details/@item.Id';">
        <td>
            <img src="/Images/View/@Html.Raw(item.ImageId)" width="100"/>
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Title)
        </td>

        @*<td>
            @Html.DisplayFor(modelItem => item.MeetingPlace)
        </td>*@

        <td>
            @Html.DisplayFor(modelItem => item.StartDate)
        </td>

        @*<td>
            @Html.ActionLink("Details", "Details", new {id = item.Id}) | @Html.ActionLink("Details2", "Details2", new { id = item.Id })
        </td>*@
    </tr>
}

</table>
