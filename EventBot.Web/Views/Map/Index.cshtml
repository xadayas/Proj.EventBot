@using System.Globalization
@model EventBot.Web.Models.LocationViewModel
@{
    double latitude= 63.863663d, longitude= 20.3268386d;
    if (Model != null)
    {
        latitude = Model.Latitude;
        longitude = Model.Longitude;
    }

}
<iframe frameborder="0" style="border: 0; width: 100%; height: 400px;"
        src="https://www.google.com/maps/embed/v1/place?key=AIzaSyCnWx0t3oWPAUiv1sxfmdSKXN7k58L4JBU&center=@latitude.ToString(CultureInfo.InvariantCulture),@longitude.ToString(CultureInfo.InvariantCulture)&zoom=18&q=@latitude.ToString(CultureInfo.InvariantCulture),@longitude.ToString(CultureInfo.InvariantCulture)"
        allowfullscreen></iframe>
